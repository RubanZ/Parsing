    $('.office_ps_select').change(function (e) {
        var selected_ps = $(this).val();
        if ( selected_ps ) {
            $('.js-cashbox').addClass('ps_hide');
            $('.js-cashbox.ps_' + selected_ps).removeClass('ps_hide');
        } else {
            $('.js-cashbox').removeClass('ps_hide');
        }
        renderOffice();
    });

    jQuery.expr[':'].Contains = function(a, i, m) {
        return jQuery(a).text().toUpperCase().indexOf(m[3].toUpperCase()) >= 0;
