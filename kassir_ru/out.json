<script>

    function renderOffice() {
        var renderOffices = [];
        $.each($('.js-cashbox:not(.ps_hide):not(.text_hide)'), function (i, e) {
            renderOffices.push(cashbox_list[$(e).data('cashbox-id')]);
        });
        reloadCashBoxMap(renderOffices, cashbox_map_params);
    }


    $('.office_ps_select').change(function (e) {
        var selected_ps = $(this).val();
        if ( selected_ps ) {
            $('.js-cashbox').addClass('ps_hide');
            $('.js-cashbox.ps_' + selected_ps).removeClass('ps_hide');
        } else {
            $('.js-cashbox').removeClass('ps_hide');
        }
        renderOffice();
    });

    jQuery.expr[':'].Contains = function(a, i, m) {
        return jQuery(a).text().toUpperCase().indexOf(m[3].toUpperCase()) >= 0;
    };

    $('input.search').change(function (e) {
        var search_text = $(this).val();
        $('.js-cashbox').addClass('text_hide');
        $(".js-cashbox:Contains('" + search_text + "')").removeClass('text_hide');
        renderOffice();
    });
</script>

